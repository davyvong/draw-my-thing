{
  "private": true,
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "nest build && npm run create-package",
    "build:clean": "rimraf dist",
    "create-package": "node scripts/create-package",
    "deploy": "sh ./scripts/deploy-heroku.sh davyvong-drawmything",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start -e \"ts-node -r ./src/config\"",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start -- --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node -r ./dist/src/config dist/src/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.5",
    "@nestjs/core": "^7.0.5",
    "@nestjs/graphql": "^7.3.9",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-fastify": "^7.0.5",
    "@nestjs/schedule": "^0.4.0",
    "apollo-server-fastify": "2.14.2",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-type-json": "^0.3.1",
    "moment": "^2.25.3",
    "mongoose": "^5.9.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.5",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/mongoose": "^5.7.7",
    "@types/node": "^13.9.3",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
